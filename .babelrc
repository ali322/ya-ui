{
    "presets": ["env", "react"],
    "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
        "transform-export-extensions"
    ],
    "env": {
        "production": {
            "presets": ["react-optimize"],
            "plugins": [
                ["transform-runtime", {
                    "helpers": false,
                    "polyfill": false,
                    "regenerator": true
                }]
            ]
        }
    }
}